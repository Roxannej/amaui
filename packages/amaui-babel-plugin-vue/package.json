{
  "name": "@amaui/babel-plugin-vue",
  "version": "1.0.0",
  "description": "Babel plugin vue",
  "repository": "https://github.com/amaui-org/amaui.git",
  "author": "Lazar <lazareric.com@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "./src/index.ts",
  "keywords": [
    "vue",
    "vue-style",
    "vue-styling",
    "css",
    "css-in-js",
    "babel",
    "babel-plugin",
    "style",
    "styling",
    "cascading-style-sheets",
    "frontend",
    "front-end",
    "backend",
    "back-end",
    "web",
    "browser",
    "javascript",
    "js",
    "typescript",
    "node",
    "nodejs",
    "library",
    "amaui",
    "utils"
  ],
  "scripts": {
    "prebuild": "rimraf build",
    "build": "cross-env ENV=babel node ../../utils/js/build.js",
